{
  "compilerOptions": {
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "target": "es2020",
    "preserveConstEnums": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  }
}